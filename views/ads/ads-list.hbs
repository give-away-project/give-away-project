<h1>Ads in the Database:  </h1>

{{#each ad}}
    <section>
        <img src="{{imageUrl}}">
        <h2>{{this.title}}</h2>
        <p>
            Description: {{this.description}} <br/> 
            <a href="/ads/{{_id}}">More details</a> |
            <a href="/ads/{{_id}}/edit">Edit</a>
        </p>

    </section>
{{/each}} 


